{
  "expo": {
    "name": "InstaSport",
    "slug": "insta-sport",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.universapp.instasport",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.universapp.instasport",
      "versionCode": 1,
      "useNextNotificationsApi": "true"
    }
  }
}
